booktitle = {2008 International Workshop on Advanced Information Systems for Enterprises},
doi = {10.1109/IWAISE.2008.18},
file = {:data/private/vxn851/papers/Slimane, Souveyet - 2008 - A Goal Driven Approach to Deal with Quality of Service as Potential Aspects.pdf:pdf},
