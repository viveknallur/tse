@ARTICLE{Agarwal2010From,
  author = {Agarwal, Vikas and Jalote, Pankaj},
  title = {{From Specification to Adaptation: An Integrated QoS-driven Approach
	for Dynamic Adaptation of Web Service Compositions}},
  journal = {2010 IEEE International Conference on Web Services},
  year = {2010},
  pages = {275--282},
  month = jul,
  abstract = {Web services composition environment is highly dynamic with new services
	being deployed, existing ones becoming unavailable or their QoS and
	other non-functional properties (cost, availability, etc.) changing
	dynamically. However, current approaches for web services composition
	and execution, such as WS-BPEL, can neither tailor the execution
	automatically as per the required QoS nor can they adapt to the highly
	dynamic environment. Though there have been some recent efforts in
	this direction they are piecemeal and insufficient. They either do
	not take user's non-functional requirements (NFRs) into consideration
	for adaptation, or there is no standard way of specifying these requirements
	for a BPEL process. In this paper, we propose an integrated approach
	for dynamically adapting web service compositions based on NFRs.
	We first give a specification for representing NFRs for each partner
	service of a BPEL process, and then describe a system that dynamically
	adapts the BPEL process based on these requirements by selecting
	suitable services at runtime. The selected services only need to
	be semantically equivalent and the system automatically takes care
	of the syntactical differences between their interfaces. We integrated
	our system in an existing BPEL engine using aspect oriented approach
	and demonstrate via experiments that it has very little performance
	overhead even at high loads.},
  doi = {10.1109/ICWS.2010.39},
  file = {:data/private/vxn851/papers/Agarwal, Jalote - 2010 - From Specification
	to Adaptation An Integrated QoS-driven Approach for Dynamic Adaptation
	of Web Service Compositions.pdf:pdf},
  isbn = {978-1-4244-8146-0},
  keywords = {-non-functional properties,adaptation,bpel,composite service,qos,web
	service,wsdl},
  publisher = {Ieee},
  url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5552775}
}

