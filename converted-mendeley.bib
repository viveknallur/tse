@ARTICLE{Ma2008Linear,
  author = {Ma, Xiaoning and Dong, Baotian},
  title = {{Linear Physical Programming-Based Approach for Web Service Selection}},
  journal = {2008 International Conference on Information Management, Innovation
	Management and Industrial Engineering},
  year = {2008},
  pages = {398--401},
  month = dec,
  abstract = {Selection of Web service according to different quality of service
	(QoS) has received much interest to support rapid and dynamic composition
	of services. Such problem is a multiple attribute decision making
	(MADM) problem and the most commonly used method is simple additive
	weight method (SAW). Weight-based approach is simple to use but with
	some limitations: these weights have no physical meaning and hard
	to set. Linear physical programming (LPP) is a new effective and
	computationally approach to overcome such limitations of SAW. In
	this paper, a LPP based approach for quality driven Web service selection
	is proposed. Firstly we give two improvements of LPP: determine proper
	initial value of convexity parameter without iterations and use the
	simple form of preference function. Then we discuss implementation
