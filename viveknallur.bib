@ARTICLE{Floch2006Using,
  author = {Floch, Jacqueline and Hallsteinsen, Svein and Stav, Erlend and Eliassen,
	Frank and Lund, Ketil and Gjorven, Eli },
  title = {Using Architecture Models for Runtime Adaptability},
  journal = {IEEE Software},
  year = {2006},
  volume = {23},
  pages = {62--70},
  number = {2},
  abstract = {Developers typically use software architecture models at design time
	to capture significant decisions about a software system's organization
	and to describe and establish a common understanding about the system's
	abstract properties. Architectural information isn't usually explicitly
	represented at runtime; developers transform and realize the architectural
	properties through runtime artifacts. Recently, the introduction
	of software platforms supporting component plug-in and dynamic binding
	has facilitated adaptation of software systems at runtime. Preserving
	the properties described by the architecture model during adaptation
	is an important task. We propose a self-adaptation approach that
